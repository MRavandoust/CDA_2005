
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/css/main.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/1df9e1fae9.js" crossorigin="anonymous"></script>
    <title>@ViewData["Title"] - Legumes</title>
</head>

<body>
    <header>
        <img src="~/legumos.jpg" />
        <nav>
            <ul>
                <li class="first"><a href="@Url.Action("Index", "Home")">ACCUEIL</a> </li>
                <li class="first"><a href="@Url.Action("Index", "Sales")">VENTES</a> </li>
                <li class="second"><a href="@Url.Action("Details", "Sales")">NOUVELLE VENTE</a></li>
                <li class="second"><a href="@Url.Action("Index", "Legumes")">LEGUMES</a></li>
                <li class="first"><a href="@Url.Action("Details", "Legumes")">NOUVEAU LEGUMES</a></li>
            </ul>
        </nav>


    </header>

    <div class="container">

        

        

        @RenderBody()


    </div>


    <footer><i class="far fa-copyright"></i> Copyright 2021 | ECF_LEGUMOS_RAVANDOUST</footer>
</body>

</html>
































